  "homepage": "https://github.com/media-network/media-portfolio#readme",
  "scripts": {
    "start": "concurrently --raw \"npm run dev-server\" \"wait-on http://localhost:3008/alive && npm run server\"",
    "server": "run.env nodemon ./src --exec babel-node --watch ./src --watch ./dist/manifest.json",
    "dev-server": "run.env nodemon ./internals/dev-server --exec babel-node --watch ./internals",
    "webpack": "webpack --config internals/webpack.babel.js",
    "build": "rm -rf ./build && babel ./src -d ./build && cp -r ./src/server/views ./build/server/views"
  }
}
