<!doctype html>
<html>
  <head>
    <title>Media CDN</title>
    <% include 'partials/meta.ect' %>
    <% include 'partials/inline-css.ect' %>
    <% if @__asset('home.css') :%>
      <link rel="stylesheet" href="<%= @__asset('home.css') %>">
    <% end %>
  </head>
  <body>
    <div class="header">
      <div class="container">
        <div class="logo">
        </div>
        <ul class="top-nav">
          <li>
            <a href="#">Features</a>
          </li>
          <li>
            <a href="#">Pricing</a>
          </li>
        </ul>
      </div>
    </div>
    <div class="section-landing section-scene">
      <div class="top-line"></div>
      <div class="bottom-line"></div>
      <div id="origin-scene" class="origin scene">
        <div data-depth="0" class="layer"></div>
        <div data-depth="0.1" class="layer"></div>
        <div data-depth="0.2" class="layer"></div>
        <div data-depth="0.4" class="layer"></div>
      </div>
      <!-- <div class="intro">
        <h1>A Simple, Fast and Reliable CDN</h1>
      </div> -->
    </div>
    <div class="section-promotion">
      <div class="container">
        <h1>A Simple, Fast and Reliable CDN</h1>
        <h2 class="double-break-after">The real-time content delivery network built for the future</h2>
        <a href="#" class="offer">
          <span>Try for free</span>
        </a>
        <span class="note">No credit card required</span>
      </div>
    </div>
    <div class="section-landing section-scene">
      <div class="top-line"></div>
      <div class="bottom-line"></div>
      <div id="target-scene" class="target scene">
        <div data-depth="0" class="layer"></div>
        <div data-depth="0.1" class="layer"></div>
        <div data-depth="0.2" class="layer"></div>
        <div data-depth="0.4" class="layer"></div>
      </div>
      <!-- <div class="intro">
        <h1>A Simple, Fast and Reliable CDN</h1>
      </div> -->
    </div>
    <script src="<%= @__asset('home.js') %>"></script>
  </body>
</html>
