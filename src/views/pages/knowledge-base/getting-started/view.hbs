{{#> layouts/default}}
  {{#*inline "content-body"}}
    <div class="container header-wrapper">
      <div class="container-center">
        {{> partials/header.hbs}}
      </div>
    </div>

    {{> pages/knowledge-base/base.hbs}}

    <div class="container knowledge-base-body">
      <div class="knowledge-base-body-menu">
        <div class="knowledge-base-body-menu-center">
          <a class="knowledge-base-body-menu-center-home" href="/knowledge-base">{{labels.feature.home_text}}</a>&#62;<span class="knowledge-base-body-menu-center-home-linked">{{labels.feature.page_title.getting_started}}</span>
        </div>
      </div>

      {{> pages/knowledge-base/post-loadding.hbs}}

      <div class="container-center knowledge-base-body-wrapper" id="knowledge-base-body-wrapper">
      </div>
    </div>

    {{> pages/knowledge-base/base-search.hbs}}

    {{> partials/footer.hbs}}
  {{/inline}}
{{/ layouts/default}}
